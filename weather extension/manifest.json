{
    "name": "Weather app",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Chrome Extension That Show You Weather :)",
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "cookies",
        "tabs",
        "activeTab",
        "declarativeContent",
        "<all_urls>",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
